        $("input[name=q]")
          .on("keydown", function(e) {
            e.stopPropagation();
          })
          .on("keyup", function(e) {
            if (e.keyCode == 13) {
              $("input.search-button").trigger("click");
            }
          });

        $("input.search-button").on("click", function(e) {
          var str = $("input#q")
            .val()
            .trim();
          console.log(str);
          if (str.length >= 3 || str.length == 0) {
            var obj = {
              page: "filemanager",
              q: (str.indexOf(":") != -1 ? "" : "name:") + str
            };
            if ($(".search-filter").data("filter"))
              obj.filter = $(".search-filter").data("filter");
            console.log("pushing", obj);
            $.bbq.pushState(obj, 2);
          } else HFN.message(__("Type at least 3 letters to search."), "error");
        });

        $(".logo a").on("click", function() {
          $(window).scrollTop(0);
        });

